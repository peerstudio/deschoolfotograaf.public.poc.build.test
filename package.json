{
	"name": "my-app4",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build:test": "vite build --mode test && cp package.json build && npm install --omit=dev --prefix build",
		"build:prod": "vite build --mode production && cp package.json build && npm install --omit=dev --prefix build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:pull": "prisma db pull",
		"db:studio": "prisma studio",
		"icons:download": "node scripts/download-icons.js",
		"icons:fetch-all": "node scripts/fetch-all-icons-from-db.js",
		"build:combell:test": "vite build --mode test && cp package.json build",
		"build": "npm install --omit=dev",
		"serve": "node ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@prisma/adapter-mssql": "^7.1.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"@types/nodemailer": "^7.0.4",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"prisma": "^7.1.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"sveltekit-superforms": "^2.29.1",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"vite": "^7.2.6",
		"zod": "^4.2.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@prisma/client": "^7.1.0",
		"@sentry/sveltekit": "^10",
		"nodemailer": "^7.0.12"
	}
}
